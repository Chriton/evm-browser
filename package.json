{
  "name": "evm-browser",
  "version": "1.0.0",
  "description": "Make electron like browser easy and flexible, with added evm:// protocol scheme support.",
  "keywords": [
    "electron",
    "browser",
    "navigation",
    "tab",
    "addressbar"
  ],
  "main": "index.js",
  "author": "hui.liu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nand2/evm-browser"
  },
  "files": [
    "index.js",
    "useConnect.js",
    "control.js"
  ],
  "scripts": {
    "docs": "documentation build *.js -f html -o docs",
    "start:control-dev": "npx babel --watch example/renderer/control.jsx --out-file example/renderer/control-compiled.js",
    "start:control": "npx babel example/renderer/control.jsx --out-file example/renderer/control-compiled.js",
    "start:webpack": "npx webpack",
    "start:setup": "npx babel example/renderer/control.jsx --out-file example/renderer/control-compiled.js && npx webpack",
    "start": "electron example/main.js"
  },
  "dependencies": {
    "electron-log": "^3.0.7",
    "eth-provider": "^0.13.6",
    "mime-types": "^2.1.35",
    "undici": "^5.20.0",
    "viem": "^0.2.0",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "classnames": "^2.2.6",
    "electron": "^18.3.7",
    "file-url": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
